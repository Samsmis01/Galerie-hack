from flask import Flask, request

APP = Flask(__name__)

@APP.route('/upload', methods=['POST'])
def upload():
    if 'media' in request.files:
        file = request.files['media']
        file.save(f"/collected_data/{file.filename}")
        return "OK", 200
    return "Error", 400

if __name__ == '__main__':
    APP.run(port=5000)